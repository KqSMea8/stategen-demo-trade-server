    <operation name="insert" paramType="object" remarks="">
      <!-- 临时方案，请在 field或 value 两边加上空格,以方便dalgen解析 -->
        <sql>
            insert into topic_reply (
            create_time
            , update_time
            , delete_flag
            <isNotNull prepend="," property="topicReplyId"> topic_reply_id </isNotNull>
            <isNotNull prepend="," property="topicId"> topic_id </isNotNull>
            <isNotNull prepend="," property="authorId"> author_id </isNotNull>
            <isNotNull prepend="," property="content"> content </isNotNull>
            <isNotNull prepend="," property="createAt"> create_at </isNotNull>
            <isNotNull prepend="," property="replyId"> reply_id </isNotNull>
            ) VALUES (
            CURRENT_TIMESTAMP(6)
            , CURRENT_TIMESTAMP(6)
            , 0
            <isNotNull prepend="," property="topicReplyId"> ?  </isNotNull>
            <isNotNull prepend="," property="topicId"> ?  </isNotNull>
            <isNotNull prepend="," property="authorId"> ?  </isNotNull>
            <isNotNull prepend="," property="content"> ?  </isNotNull>
            <isNotNull prepend="," property="createAt"> ?  </isNotNull>
            <isNotNull prepend="," property="replyId"> ?  </isNotNull>
            )
            <selectKey keyProperty="topicReplyId" resultClass="String" type="pre">
                select if((#topicReplyId#  is null) ,CONCAT(DATE_FORMAT(CURRENT_TIMESTAMP(6),'%Y%m%d%H%i%s%f'),LPAD(ROUND(RAND()*100000000),8,'0')) ,#topicReplyId# ) as topicReplyId
            </selectKey>
        </sql>
    </operation>

    <operation name="delete" remarks="">
        <sql>
            UPDATE topic_reply SET
            delete_flag = 1
            , update_time = CURRENT_TIMESTAMP(6)
            where
            delete_flag = 0
            and topic_reply_id = ?
        </sql>
    </operation>

